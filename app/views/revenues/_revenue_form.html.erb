<tr id="rev-<%= revenue.id %>">
  <th scope="row"><%= revenue.id %></th>
  <td><%= revenue.created_at.strftime('%Y/%m/%d') %></td>
  <td><%= (revenue.amount).to_s(:delimited) %>円</td>
  <td class="text-center"><%= revenue.head_count %></td>
  <td colspan="2" class="fontweight-700"><%= revenue.memo %></td>
  <td><%= link_to "削除", revenue_path(revenue), method: :delete, remote: true, class: 'btn-del small-btn' %></td>
</tr>